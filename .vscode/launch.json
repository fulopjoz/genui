{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "autoStartBrowser": false,
            "justMyCode": true,
            "env": {
                "DJANGO_SETTINGS_MODULE": "genui.settings.debug"
            }
        }
    ]
}
